		<parameter name="branchCode" class="java.lang.String"/>
		<parameter name="applicationNo" class="java.lang.String"/>
		<queryString>
			<![CDATA[select rownum sno,DESCRIPTION,
trim(to_char(round(COMMODITY_EXCESS_PREMIUM,(select rfactor from currency_master cm
where  cm.CURRENCY_ID=mrd.CURRENCY_ID and rownum=1)),'999G999G999G999G999D999')) ||'  '||
(select SHORT_NAME from currency_master cm where  cm.CURRENCY_ID=mrd.CURRENCY_ID and rownum=1) Excess
from marine_result_details mrd where application_no='13352']]>
		</queryString>
		<field name="SNO" class="java.math.BigDecimal"/>
		<field name="DESCRIPTION" class="java.lang.String"/>
		<field name="EXCESS" class="java.lang.String"/>
	</subDataset>
