	<parameter name="status" class="java.lang.String"/>
	<queryString>
		<![CDATA[select pos.POLICY_NO, convert(varchar,pos.inception_date,103)inception_date,per.email,per.ADDRESS1,per.
      ADDRESS2,per.POBOX,per.EMIRATE,per.TELEPHONE,per.MOBILE,per.FAX,per.
      COMPANY_NAME,per.DOB,isnull(per.FIRST_NAME,per.COMPANY_NAME)FIRST_NAME,per. LAST_NAME,per.NATIONALITY,per.TITLE,per.login_id,convert(varchar,th.EFFECTIVE_DATE,103)EFFECTIVE_DATE,
      convert(varchar,th.INSURANCE_END_DATE,103)INSURANCE_END_DATE,pos.PREMIUM,pos.
      EXCESS_PREMIUM,pos.OVERALL_PREMIUM,pos.EXCESS_SIGN,th.scheme_cover,per.CUSTOMER_SOURCE,per.OCCUPATION,
      CONVERT (VARCHAR, isnull (pos.DEBIT_NOTE_DATE, pos.EFFECTIVE_DATE), 103)
         + ' '
         + CONVERT (VARCHAR (8),
                    isnull (pos.DEBIT_NOTE_DATE, pos.EFFECTIVE_DATE),
                    108) seal_date,
         isnull (pos.POLICY_FEE, '0')POLICY_FEE,convert(VARCHAR,pos.INCEPTION_DATE, 103) startDate,th.QUOTE_NO
      from HOME_POSITION_MASTER pos,personal_info per,TRAVEL_HEADER th,
      TRAVEL_DETAIL td
      where pos.QUOTE_NO = $P{quoteNo}
      and per.customer_id=pos.customer_id
      and th.CUSTOMER_ID = pos.CUSTOMER_ID
      and  th.STATUS = 'Y'
      and th.QUOTE_NO=cast(pos.QUOTE_NO as varchar)
      and td.QUOTE_NO=cast(pos.QUOTE_NO as varchar)]]>
	</queryString>
