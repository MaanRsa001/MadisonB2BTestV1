	<subDataset name="InsuredDetail" uuid="035da5c8-741b-438f-a55b-1c19d4675be0">
		<queryString>
			<![CDATA[SELECT   TD.PASSENGER_NAME,
           CONVERT (varchar, TD.DOB, 103)DOB,
           CD.DETAIL_NAME,
           TD.GENDER,
           TD.NATIONALITY,
           td.age,
           isnull (td.PREMIUM, 0)PREMIUM
    FROM   TRAVEL_DETAIL TD, CONSTANT_DETAIL CD
   WHERE       TD.QUOTE_NO = '100687'
           AND CD.CATEGORY_DETAIL_ID = TD.RELATION
           AND CD.CATEGORY_ID = '53'
           AND CD.BRANCH_CODE = '01'
           AND TD.AMEND_ID = (SELECT   MAX (AMEND_ID)
                                FROM   TRAVEL_DETAIL
                               WHERE   QUOTE_NO = TD.QUOTE_NO AND STATUS = 'Y')
ORDER BY   TD.SERIAL_NO]]>
		</queryString>
		<field name="PASSENGER_NAME" class="java.lang.String"/>
		<field name="DOB" class="java.lang.String"/>
		<field name="DETAIL_NAME" class="java.lang.String"/>
		<field name="GENDER" class="java.lang.String"/>
		<field name="NATIONALITY" class="java.lang.String"/>
		<field name="age" class="java.lang.Integer"/>
		<field name="PREMIUM" class="java.math.BigDecimal"/>
	</subDataset>
