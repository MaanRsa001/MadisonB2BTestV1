	 <package name="intercept" extends="struts-default,tiles-default,json-default">
		  	<result-types>
				<result-type name="tiles" class="org.apache.struts2.views.tiles.TilesResult" />
			</result-types>
			<interceptors>
       			<interceptor name="noCacheIntercept" class="com.maan.common.interceptor.NoCacheInterceptor" />
				<interceptor name="SessionIntercept" class="com.maan.common.interceptor.SessionInterceptor" />
				<interceptor-stack name="noCacheStack">
					<interceptor-ref name="noCacheIntercept" />				
	     			<interceptor-ref name="prepare"/>
					<interceptor-ref name="defaultStack">
						<param name="exception.logEnabled">true</param>
						<param name="exception.logLevel">ERROR</param>
					</interceptor-ref>
					<interceptor-ref name="token">
						<param name="excludeMethods">*</param>
					</interceptor-ref>
					<interceptor-ref name="fileUpload">
						<param name="maximumSize">52428800</param>
						<param name="contentType">application/octet-stream</param>
					</interceptor-ref>
					<interceptor-ref name="actionMappingParams" />
					<interceptor-ref name="params"> 
						<param name="excludeParams">dojo\..*,^struts\..*</param>
					</interceptor-ref>
					<interceptor-ref name="conversionError" />
					<interceptor-ref name="validation"/>
					<interceptor-ref name="workflow"/>
				</interceptor-stack>
				<interceptor-stack name="SessionCheckStack">
					<interceptor-ref name="SessionIntercept" />
					<interceptor-ref name="noCacheStack" />
				</interceptor-stack>
       		</interceptors>
       		<default-interceptor-ref name="SessionCheckStack"></default-interceptor-ref>
       		<global-results>
       			<result name="session" type="tiles">SessionExpired</result>
       			<result name="invalid.token" type="tiles">SessionExpired</result>
       			<result name="unhandledException" type="tiles">SessionExpired</result>
       		</global-results>
       		<global-exception-mappings>
       			<exception-mapping result="ExceptionHandler" exception="java.lang.Exception" />
       		</global-exception-mappings>
       	</package>
